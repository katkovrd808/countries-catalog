scalar Date

type Query {
    country(isoCode: String!): Country!
    countries(page: Int, size: Int): CountryConnection
}

type Mutation {
    addCountry(input: CountryInput): Country!
    updateCountry(input: CountryInput): Country!
}

type Country {
    id: ID!
    name: String!
    isoCode: String!
    coordinates: [[[[Float!]!]!]!]!
}

input CountryInput {
    name: String
    isoCode: String!
    coordinates: [[[[Float!]!]!]!]!
}